{
  "name": "kmen-fondrozvoje",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"firebase emulators:start --project=demo-kmen-fondrozvoje\" \"./node_modules/.bin/tsx --tsconfig ./tsconfig.scripts.json ./scripts/populate-emulator.ts\" \"next dev --turbopack\"",
    "dev:next": "next dev --turbopack",
    "dev:emulator": "firebase emulators:start --project=demo-kmen-fondrozvoje",
    "populate-emulator": "./node_modules/.bin/tsx --tsconfig ./tsconfig.scripts.json ./scripts/populate-emulator.ts",
    "dev:fresh": "firebase emulators:start --project=demo-kmen-fondrozvoje --import=./emulator-data --export-on-exit",
    "dev:reset": "rimraf ./.firebase-emulator-data && npm run dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "dev:debug": "NODE_OPTIONS='--inspect' next dev"
  },
  "dependencies": {
    "@radix-ui/react-alert-dialog": "^1.1.6",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-collapsible": "^1.1.3",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-progress": "^1.1.2",
    "@radix-ui/react-scroll-area": "^1.2.3",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slider": "^1.2.3",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-switch": "^1.1.3",
    "@radix-ui/react-tabs": "^1.1.3",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@tanstack/react-query": "^5.67.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "firebase": "^11.4.0",
    "firebase-admin": "^13.2.0",
    "firebase-functions": "^6.3.2",
    "framer-motion": "^12.4.10",
    "lucide-react": "^0.476.0",
    "next": "15.1.7",
    "next-themes": "^0.4.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sonner": "^2.0.1",
    "tailwind-merge": "^3.0.2",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.2",
    "eslint": "^9",
    "eslint-config-next": "15.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.5.3",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3",
    "typescript": "^5"
  }
}
